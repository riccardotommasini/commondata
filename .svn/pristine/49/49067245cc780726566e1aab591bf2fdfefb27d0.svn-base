package org.webdatacommons.webgraph.process.shrink;

/**
 * Transformation level according to shrinked index.
 * <ul>
 * 	<li>PLD - Pay-Level-Domain e.g. uni-mannheim.de</li>
 * 	<li>SD - Complete Sub-Domain e.g. dws.informatik.uni-mannheim.de</li>
 *  <li>SD1 - First Sub-Domain e.g. informatik.uni-mannheim.de</li>
 * </ul>
 * 
 * @author Robert Meusel (robert@informatik.uni-mannheim.de)
 *
 */
public enum TransformationLevel {
	
	PLD,
	SD,
	SD1;

}
